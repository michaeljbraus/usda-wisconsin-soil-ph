read.csv("2019-08-22-hatch-ph-standards-0.1MCaCl2-compiled.csv", header=T)
dat <- read.csv("2019-08-22-hatch-ph-standards-0.1MCaCl2-compiled.csv", header=T)
str(dat)
library(ggplot2)
p <- ggplot(dat, aes(x = time, y = pH, color = sample))
p + facet_wrap(~date) + geom_smooth() + theme_bw()
p + geom_point() + facet_wrap(~date) + geom_smooth() + theme_bw()
p + geom_point() + facet_wrap(~date, scales="free") + geom_smooth() + theme_bw()
p + geom_point() + facet_wrap(~date, scales="free+x") + geom_smooth() + theme_bw()
p + geom_point() + facet_wrap(~date, scales="free_x") + geom_smooth() + theme_bw()
p + geom_point() + facet_wrap(~date, scales="free_x") + geom_smooth(method = "lm") + theme_bw()
p + geom_point() + facet_wrap(~date, scales="free_x") + geom_smooth(method = "auto") + theme_bw()
p + geom_point() + geom_line() + facet_wrap(~date, scales="free_x") + theme_bw()
p + geom_point() + facet_wrap(~date, scales="free_x") + theme_bw()
p <- ggplot(dat, aes(x = time, y = 10^-pH, color = sample))
p + geom_point() + facet_wrap(~date, scales="free_x") + theme_bw()
dat.standard <- subset(dat, sample=="standard")
p <- ggplot(dat.standard, aes(x = time, y = 10^-pH, color = sample))
p + geom_point() + facet_wrap(~date, scales="free_x") + theme_bw()
